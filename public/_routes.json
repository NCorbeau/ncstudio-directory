{
  "version": 1,
  "include": ["/*"],
  "exclude": ["/api/*"],
  "routes": [
    {
      "src": "/",
      "dest": "/french-desserts/index.html",
      "has": [
        {
          "type": "host",
          "value": "french-desserts.ncstudio.click"
        }
      ]
    },
    {
      "src": "/",
      "dest": "/dog-parks-warsaw/index.html",
      "has": [
        {
          "type": "host",
          "value": "dog-parks-warsaw.ncstudio.click"
        }
      ]
    },
    {
      "src": "/",
      "dest": "/directory-selector/index.html"
    },
    {
      "src": "/:directory",
      "dest": "/:directory/index.html"
    },
    {
      "src": "/:directory/",
      "dest": "/:directory/index.html"
    },
    {
      "src": "/:directory/*",
      "dest": "/:directory/:splat"
    },
    {
      "src": "/api/search",
      "dest": "/api/search.js"
    },
    {
      "src": "/api/webhook",
      "dest": "/api/webhook.js"
    },
    {
      "src": "/_astro/*.js",
      "headers": {
        "Content-Type": "application/javascript"
      },
      "continue": true
    },
    {
      "src": "/*.js",
      "headers": {
        "Content-Type": "application/javascript"
      },
      "continue": true
    },
    {
      "src": "/_astro/*.css",
      "headers": {
        "Content-Type": "text/css"
      },
      "continue": true
    },
    {
      "src": "/*.css",
      "headers": {
        "Content-Type": "text/css"
      },
      "continue": true
    },
    {
      "src": "/_astro/*.js",
      "dest": "/_astro/:splat.js"
    }
  ]
}